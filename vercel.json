{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1",
      "headers": {
        "Set-Cookie": "HttpOnly; Secure"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
